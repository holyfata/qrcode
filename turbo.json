{
    "$schema": "https://turbo.build/schema.json",
    "tasks": {
        "build": {
            "outputs": ["dist/**"],
            "dependsOn": [
                "@holyfata/qrcode-vanilla#build",
                "@holyfata/qrcode-react#build",
                "@holyfata/qrcode-vue#build"
            ]
        },
        "build:pkg": {
            "dependsOn": [
                "@holyfata/qrcode-vanilla#build",
                "@holyfata/qrcode-react#build",
                "@holyfata/qrcode-vue#build"
            ]
        },
        "@holyfata/qrcode-vanilla#build": {
            "outputs": ["dist/**"]
        },
        "@holyfata/qrcode-react#build": {
            "outputs": ["dist/**"],
            "dependsOn": [
                "@holyfata/qrcode-vanilla#build"
            ]
        },
        "@holyfata/qrcode-vue#build": {
            "outputs": ["dist/**"],
            "dependsOn": [
                "@holyfata/qrcode-vanilla#build"
            ]
        },
        "dev": {
            "dependsOn": [
                "@holyfata/qrcode-react#build",
                "@holyfata/qrcode-vue#build"
            ]
        }
    }
}
